{"ast":null,"code":"export default {};","map":{"version":3,"names":[],"sources":["src/components/Todo/ListTasks.vue"],"sourcesContent":["<template>\n  <v-list class=\"pt-0\" flat>\n      <div v-for=\"task in $store.state.tasks\" :key=\"task.id\">\n        <v-list-item\n          @click=\"$store.commit('doneTask', task.id)\"\n          :class=\"{ 'blue lighten-5': task.done }\"\n        >\n          <template v-slot:default>\n            <v-list-item-action>\n              <v-checkbox :input-value=\"task.done\" color=\"primary\"></v-checkbox>\n            </v-list-item-action>\n\n            <v-list-item-content>\n              <v-list-item-title\n                :class=\"{ 'text-decoration-line-through': task.done }\"\n                >{{ task.title }}</v-list-item-title\n              >\n            </v-list-item-content>\n            <v-list-item-action>\n              <v-btn @click.stop=\"$store.commit('deleteTask', task.id)\" icon>\n                <v-icon color=\"primary lighten-1\">mdi-trash-can-outline</v-icon>\n              </v-btn>\n            </v-list-item-action>\n          </template>\n        </v-list-item>\n        <v-divider></v-divider>\n      </div>\n    </v-list>\n</template>\n\n<script>\nexport default {\n\n}\n</script>\n\n<style>\n\n</style>"],"mappings":"AA+BA,gBAEA"},"metadata":{},"sourceType":"module","externalDependencies":[]}