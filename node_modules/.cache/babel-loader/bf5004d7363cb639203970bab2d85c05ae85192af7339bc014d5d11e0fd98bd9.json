{"ast":null,"code":"export default {};","map":{"version":3,"names":[],"sources":["src/components/Todo/Task.vue"],"sourcesContent":["<template>\n  <div>\n    <v-list-item\n      @click=\"$store.commit('doneTask', task.id)\"\n      :class=\"{ 'blue lighten-5': task.done }\"\n    >\n      <template v-slot:default>\n        <v-list-item-action>\n          <v-checkbox :input-value=\"task.done\" color=\"primary\"></v-checkbox>\n        </v-list-item-action>\n\n        <v-list-item-content>\n          <v-list-item-title\n            :class=\"{ 'text-decoration-line-through': task.done }\"\n            >{{ task.title }}</v-list-item-title\n          >\n        </v-list-item-content>\n        <v-list-item-action>\n          <v-btn @click.stop=\"$store.commit('deleteTask', task.id)\" icon>\n            <v-icon color=\"primary lighten-1\">mdi-trash-can-outline</v-icon>\n          </v-btn>\n        </v-list-item-action>\n      </template>\n    </v-list-item>\n    <v-divider></v-divider>\n  </div>\n</template>\n\n<script>\nexport default {};\n</script>\n\n<style></style>\n"],"mappings":"AA6BA"},"metadata":{},"sourceType":"module","externalDependencies":[]}